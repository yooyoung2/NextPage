{
  "bootstrap": {
    "src": ["dist/js/bootstrap.min.js", "dist/js/bootstrap.min.js.map"],
    "dest": "bootstrap"
  },
  "prismjs": {
    "src": ["prism.js", "themes/prism.css"],
    "dest": "prism"
  },
  "is_js": {
    "src": "is.min.js",
    "dest": "is"
  },
  "anchor-js": {
    "src": "anchor.min.js",
    "dest": "anchorjs"
  },
  "@fortawesome/fontawesome-free": {
    "src": "js/all.min.js",
    "dest": "fontawesome"
  },
  "isotope-layout": {
    "src": ["dist/isotope.pkgd.min.js"],
    "dest": "isotope-layout"
  },

  "isotope-packery": {
    "src": ["packery-mode.pkgd.min.js"],
    "dest": "isotope-packery"
  },
  "glightbox": {
    "src": ["dist/js/glightbox.min.js", "dist/css/glightbox.min.css"],
    "dest": "glightbox"
  },
  "countup.js": {
    "src": "dist/countUp.umd.js",
    "dest": "countup"
  },
  "@popperjs/core": {
    "src": ["dist/umd/popper.min.js", "dist/umd/popper.min.js.map"],
    "dest": "popper"
  },
  "rellax": {
    "src": ["rellax.min.js"],
    "dest": "rellax"
  },
  "overlayscrollbars": {
    "src": ["js/OverlayScrollbars.min.js", "css/OverlayScrollbars.min.css"],
    "dest": "overlayscrollbars"
  },
  "bigpicture": {
    "src": "dist/BigPicture.js",
    "dest": "bigpicture"
  },

  "imagesloaded": {
    "src": "imagesloaded.pkgd.min.js",
    "dest": "imagesloaded"
  },
  "typed.js": {
    "src": "lib/typed.js",
    "dest": "typed.js"
  },
  "lodash": {
    "src": "lodash.min.js",
    "dest": "lodash"
  },
  "progressbar.js": {
    "src": ["dist/progressbar.min.js", "dist/progressbar.min.js.map"],
    "dest": "progressbar"
  },
  "hamburgers": {
    "src": ["dist/hamburgers.min.css"],
    "dest": "hamburgers"
  },
  "remodal": {
    "src": ["/dist/*.*"],
    "dest": "remodal"
  },
  "html-beautify": {
    "src": ["/dist/*.*"],
    "dest": "html-beautify"
  }
