mixin Navigation-4(btnOutline)
	nav.navbar.navbar-expand-lg.py-2&attributes(attributes)
		.container
			a.navbar-brand(href=`${CWD}index.html`) Slick
			button.navbar-toggler.hamburger.hamburger--emphatic(type='button' data-bs-toggle='collapse' data-bs-target='#navbarSupportedContent' aria-controls='navbarSupportedContent' aria-expanded='false' aria-label='Toggle navigation')
				span.hamburger-box
					span.hamburger-inner
			.collapse.navbar-collapse#navbarSupportedContent
				ul.navbar-nav.ms-auto.mb-2.mb-lg-0
					li.nav-item
						a.nav-link(aria-current='page' href='#!') Tour
					li.nav-item
						a.nav-link(href='#!') Feature
					li.nav-item
						a.nav-link(href='#!') Pricing
					li.nav-item
						a.nav-link(href='#!') Contact
					li.nav-item.d-flex.align-items-center.ms-lg-3
						a.nav-link(href='#!').px-2
							span.fab.fa-instagram
						a.nav-link(href='#!').px-2.ms-0
							span.fab.fa-twitter
						a.nav-link(href='#!').px-2.ms-0
							span.fab.fa-linkedin-in

				ul.navbar-nav.ms-auto
					li
						a.btn.rounded-pill.btn-sm.w-100( href='#!' class=`btn-outline-${btnOutline}` )
							span.fab.fa-apple.me-2(data-fa-transform='grow-6')
							| Buy on App Store				