							li
								a(href="#").link-600 Portfolio
							li.mt-1
								a(href="#").link-600 Work
							li.mt-1
								a(href="#").link-600 Brands
					.col-12.d-sm-none
					.col-4.col-sm-3.mt-4.mt-sm-0
						h6.text-1000.fs-0.fw-semi-bold.mb-3 Resources
						ul.list-unstyled.ps-0.font-base
							li
								a(href="#").link-600 Privacy
							li.mt-1
								a(href="#").link-600 Support
							li.mt-1
