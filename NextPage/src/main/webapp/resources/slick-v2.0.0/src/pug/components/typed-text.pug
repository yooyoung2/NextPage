extends ../layouts/LayoutComponent
include ../mixins/common/ComponentLink

prepend scripts 
	script(src=`${CWD}vendors/typed.js/typed.js`)
	
append variables 
	- var pageTitle = 'Typed Text';

block component
	+ComponentLink({
		text:'For more details see, ',
		linkText: 'Typed Js Documentation',
		link: `https://mattboldt.com/demos/typed-js/`
	})
	h5 Example
	+ComponentCard
		h4.fw-light.overflow-hidden A smarter way to tell about 
			span.fw-semi-bold.ms-1(data-typed-text!=["new features.", "product updates.", "a new idea.", "an announcment."])

	h5.mt-4 JavaScript
	+ComponentCardLib
		script(src="vendors/typed.js/typed.js")
